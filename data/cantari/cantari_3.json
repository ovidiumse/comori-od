[
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Comori Nemuritoare",
    "verses": [
      [
        {
          "style": [],
          "text": "Comori nemuritoare"
        }
      ],
      [
        {
          "style": [],
          "text": "nu-s multe pe p\u0103m\u00eent, "
        }
      ],
      [
        {
          "style": [],
          "text": "dar ce nepre\u0163uire"
        }
      ],
      [
        {
          "style": [],
          "text": "au cele ce mai s\u00eent! "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Prive\u015fti aceste ziduri"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi troi\u0163e \u015fi ii: "
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi dup\u0103-at\u00eetea veacuri"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u00eent \u015fi-ast\u0103zi tot mai vii. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Bisericu\u0163e zvelte"
        }
      ],
      [
        {
          "style": [],
          "text": "cu t\u00eemplele pe cer"
        }
      ],
      [
        {
          "style": [],
          "text": "ne s\u00eent \u015fi vor r\u0103m\u00eene"
        }
      ],
      [
        {
          "style": [],
          "text": "comori ce-n veci nu pier. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Proverbele l\u0103sate"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u00eent st\u00eelpi nemuritori"
        }
      ],
      [
        {
          "style": [],
          "text": "pe drumu-n\u0163elepciunii"
        }
      ],
      [
        {
          "style": [],
          "text": "spre vecii urm\u0103tori. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "\u015ei Anele din ziduri, "
        }
      ],
      [
        {
          "style": [],
          "text": "Manolii din izvor"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi jertfele din toate"
        }
      ],
      [
        {
          "style": [],
          "text": "ne sf\u00ee\u015fie \u015fi dor. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "\u015ei lacrime amare"
        }
      ],
      [
        {
          "style": [],
          "text": "ne \u015firoaie t\u0103cut"
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103 s\u00eentem prea nevrednici"
        }
      ],
      [
        {
          "style": [],
          "text": "de-a\u015fa m\u0103re\u0163 trecut. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Bisericu\u0163\u0103 transilvan\u0103",
    "verses": [
      [
        {
          "style": [],
          "text": "Bisericu\u0163\u0103 transilvan\u0103, "
        }
      ],
      [
        {
          "style": [],
          "text": "tu pari un psalm sfios \u015fi sf\u00eent"
        }
      ],
      [
        {
          "style": [],
          "text": "uitat de \u00eengeri pe-o spr\u00eencean\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "mai mult de cer ca de p\u0103m\u00eent. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce sfin\u0163i \u0163i-au m\u0103iestrit fiin\u0163a, "
        }
      ],
      [
        {
          "style": [],
          "text": "din ce divini stejari \u015fi brazi, "
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103 tu ne-nve\u015fnice\u015fti credin\u0163a"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi dragostea ne-o \u0163ii \u015fi azi? "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce Soare \u0163i-a sfin\u0163it aura"
        }
      ],
      [
        {
          "style": [],
          "text": "din v\u00eerful unghiului divin, "
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103 \u015fi-azi lumina \u015fi c\u0103ldura"
        }
      ],
      [
        {
          "style": [],
          "text": "\u00eentregi, \u00een noi, din El ne vin? "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce tain\u0103-i ori\u015fice icoan\u0103, "
        }
      ],
      [
        {
          "style": [],
          "text": "ce jertf\u0103-i ori\u015ficare dor, "
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103-n orice zid, c\u00eend pl\u00eenge-o An\u0103, "
        }
      ],
      [
        {
          "style": [],
          "text": "moare-n Manole-ntr-un izvor! "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ai numai lacrimi \u015fi iubire"
        }
      ],
      [
        {
          "style": [],
          "text": "la orice-ncheieturi de trunchi, "
        }
      ],
      [
        {
          "style": [],
          "text": "spre sf\u00eenta ta alc\u0103tuire"
        }
      ],
      [
        {
          "style": [],
          "text": "nu pot veni numa-n genunchi... "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "... Bisericu\u0163\u0103 transilvan\u0103, "
        }
      ],
      [
        {
          "style": [],
          "text": "azi e\u015fti un grai t\u0103cut \u015fi fr\u00eent"
        }
      ],
      [
        {
          "style": [],
          "text": "de-n\u015ftiin\u0163are \u015fi dojan\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103 noi uit\u0103m un leg\u0103m\u00eent!... "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Nemuritoarele Comori",
    "verses": [
      [
        {
          "style": [],
          "text": "Nemuritoarele comori"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u00eent m\u0103rturie vie"
        }
      ],
      [
        {
          "style": [],
          "text": "acelor ce, nemuritori, "
        }
      ],
      [
        {
          "style": [],
          "text": "s-au dus \u00een ve\u015fnicie. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Orice troi\u0163\u0103 \u015fi izvor"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi piatr\u0103 \u015fi c\u0103rare"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u00eent semne ce ne spun de-a lor"
        }
      ],
      [
        {
          "style": [],
          "text": "eroic\u0103 n\u0103l\u0163are. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "C\u00eend m\u0103re\u0163ia le-o sim\u0163im"
        }
      ],
      [
        {
          "style": [],
          "text": "cum umple \u00een\u0103l\u0163imea, "
        }
      ],
      [
        {
          "style": [],
          "text": "ne-ascundem, nici s\u0103 nu ne \u015ftim -"
        }
      ],
      [
        {
          "style": [],
          "text": "Urma\u015fii lor - micimea. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce mari p\u0103rin\u0163i ne-au ctitorit"
        }
      ],
      [
        {
          "style": [],
          "text": "aceste-a\u015fez\u0103minte! "
        }
      ],
      [
        {
          "style": [],
          "text": "Cu ce m\u00eeine sfinte le-au zidit"
        }
      ],
      [
        {
          "style": [],
          "text": "pe cer \u015fi pe morminte! "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ei n-au murit, ci doar s-au dus, "
        }
      ],
      [
        {
          "style": [],
          "text": "l\u0103s\u00eend Masa T\u0103cerii -"
        }
      ],
      [
        {
          "style": [],
          "text": "prin Poarta de S\u0103rut \u00een sus -"
        }
      ],
      [
        {
          "style": [],
          "text": "pe treptele-nvierii. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce grav e chipul lor t\u0103c\u00eend"
        }
      ],
      [
        {
          "style": [],
          "text": "\u00een trecerea divin\u0103; "
        }
      ],
      [
        {
          "style": [],
          "text": "solemni, ca \u00eengerii urc\u00eend"
        }
      ],
      [
        {
          "style": [],
          "text": "coloana de lumin\u0103... "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Coloana infinit\u0103",
    "verses": [
      [
        {
          "style": [],
          "text": "Coloana infinit\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "noi ne-o privim aievea -"
        }
      ],
      [
        {
          "style": [],
          "text": "n\u0103l\u0163\u00eendu-se sl\u0103vit\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "din Cozii \u015fi Suceve. "
        }
      ],
      [
        {
          "style": [],
          "text": "Ca Iacov \u00een pustie"
        }
      ],
      [
        {
          "style": [],
          "text": "vis\u0103m sl\u0103vita Scar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "cu \u00eengeri care suie"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi \u00eengeri ce coboar\u0103. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Un schimb solemn se face"
        }
      ],
      [
        {
          "style": [],
          "text": "pe scara ne-ntrerupt\u0103: "
        }
      ],
      [
        {
          "style": [],
          "text": "cei vechi se duc spre Pace, "
        }
      ],
      [
        {
          "style": [],
          "text": "cei noi revin spre Lupt\u0103. "
        }
      ],
      [
        {
          "style": [],
          "text": "Spre Por\u0163ile M\u0103ririi"
        }
      ],
      [
        {
          "style": [],
          "text": "se urc\u0103 \u015fi coboar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "Eroii R\u0103spl\u0103tirii"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-ai Jertfei pentru \u0162ar\u0103. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Pl\u00eeng\u00eend, privim pe primii"
        }
      ],
      [
        {
          "style": [],
          "text": "n\u0103l\u0163\u00eendu-se \u00een Soare"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi \u00eei c\u00eent\u0103m mul\u0163imii"
        }
      ],
      [
        {
          "style": [],
          "text": "spre noi cobor\u00eetoare, "
        }
      ],
      [
        {
          "style": [],
          "text": "rug\u00eendu-ne fierbinte"
        }
      ],
      [
        {
          "style": [],
          "text": "spre Cel din V\u00eerf de Scar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "mai mul\u0163i a fi nainte"
        }
      ],
      [
        {
          "style": [],
          "text": "acei care coboar\u0103. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Nemurirea",
    "verses": [
      [
        {
          "style": [],
          "text": "Nemurirea, de c\u00eend \u015ftim"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-am trezit c\u0103 ne-o suim"
        }
      ],
      [
        {
          "style": [],
          "text": "rost cinstit s\u0103-nve\u015fnicim. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Plug cu boi \u00een brazde r\u00eeu"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-a dat holde p\u00een\u0103-n br\u00eeu"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-a \u0163inut \u0163ara de gr\u00eeu. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Baci cu \u015fubele de l\u00een\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-au m\u00eenat turmele-n st\u00een\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-au f\u0103cut \u0163ara st\u0103p\u00een\u0103. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "M\u0103rgineni sun\u00eend tilinci"
        }
      ],
      [
        {
          "style": [],
          "text": "cu i\u0163ari \u015fi cu opinci"
        }
      ],
      [
        {
          "style": [],
          "text": "le-au dus mun\u0163ilor juninci. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Mo\u0163i cu cercuri \u015fi ciubare"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-au purtat din zare-n zare"
        }
      ],
      [
        {
          "style": [],
          "text": "doinele nemuritoare. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Carele cu boi pl\u0103vani"
        }
      ],
      [
        {
          "style": [],
          "text": "duc istoria prin ani"
        }
      ],
      [
        {
          "style": [],
          "text": "de la daci peste romani. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "\u015ei-n \u0163\u0103r\u00een\u0103 sub ruini"
        }
      ],
      [
        {
          "style": [],
          "text": "ni-s p\u0103rin\u0163ii mor\u0163i divini"
        }
      ],
      [
        {
          "style": [],
          "text": "singurele r\u0103d\u0103cini. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Iar deasupra tuturor"
        }
      ],
      [
        {
          "style": [],
          "text": "Cerul nostru numai dor"
        }
      ],
      [
        {
          "style": [],
          "text": "singurul \u00eendur\u0103tor... "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "La Masa Asta a T\u0103cerii",
    "verses": [
      [
        {
          "style": [],
          "text": "La Masa Asta a T\u0103cerii"
        }
      ],
      [
        {
          "style": [],
          "text": "acum Maestrul ne-a adus"
        }
      ],
      [
        {
          "style": [],
          "text": "cu El \u015fi-ai Lui cei Doisprezece, "
        }
      ],
      [
        {
          "style": [],
          "text": "al treisprezecelea s-a dus. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Lua\u0163i Prezentul \u015fi-nsu\u015fi\u0163i-l "
        }
      ],
      [
        {
          "style": [],
          "text": "cu Sensul lui cel str\u0103lucit"
        }
      ],
      [
        {
          "style": [],
          "text": "a\u015fa voi ve\u0163i r\u0103m\u00eene-n Sine"
        }
      ],
      [
        {
          "style": [],
          "text": "iar El \u00een voi des\u0103v\u00eer\u015fit. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Lua\u0163i Trecutul \u015fi gusta\u0163i-l"
        }
      ],
      [
        {
          "style": [],
          "text": "cu tot ce-n El e grav \u015fi sf\u00eent"
        }
      ],
      [
        {
          "style": [],
          "text": "cu El \u00een voi \u015fi voi \u00een Sine"
        }
      ],
      [
        {
          "style": [],
          "text": "ve\u0163i fi-n Eternul Leg\u0103m\u00eent. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "De nu v\u0103 \u00eensu\u015fi\u0163i V\u0103zutul"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi Nev\u0103zutu-n acest fel"
        }
      ],
      [
        {
          "style": [],
          "text": "nu ve\u0163i vedea Eternitatea, "
        }
      ],
      [
        {
          "style": [],
          "text": "n-ajunge\u0163i ultimul ei \u0162el... "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Maestre, ceasu-acesta-i unic"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi grav \u015fi-nfrico\u015fat de sf\u00eent, "
        }
      ],
      [
        {
          "style": [],
          "text": "prime\u015fte-ne \u015fi ne-mpreun\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "cu Sensu-acestui Leg\u0103m\u00eent. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Solemni, t\u0103cu\u0163i - \u00eentoarcem roat\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "gust\u00eend din P\u00eeine \u015fi Potir"
        }
      ],
      [
        {
          "style": [],
          "text": "nenum\u0103rata \u0163\u0103rii gloat\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "de-a lungul vremurilor \u015fir... "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Comori Nemuritoare",
    "verses": [
      [
        {
          "style": [],
          "text": "Comori Nemuritoare"
        }
      ],
      [
        {
          "style": [],
          "text": "ni-s Soarele \u015fi Glia, "
        }
      ],
      [
        {
          "style": [],
          "text": "din ele toarcem timpul"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi \u0163esem ve\u015fnicia; "
        }
      ],
      [
        {
          "style": [],
          "text": "spre Soare n\u0103l\u0163\u0103m fruntea, "
        }
      ],
      [
        {
          "style": [],
          "text": "spre Glie plec\u0103m fa\u0163a, "
        }
      ],
      [
        {
          "style": [],
          "text": "din c\u00eentec \u015fi sudoare"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u0103-nve\u015fnicim via\u0163a. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "\u015ei Piatra \u015fi Izvorul"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-ascund \u015fi ne arat\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi taina \u015fi minunea"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi crucea-ns\u00eengerat\u0103; "
        }
      ],
      [
        {
          "style": [],
          "text": "de Dincoace de Glie"
        }
      ],
      [
        {
          "style": [],
          "text": "spre Dincolo de Soare"
        }
      ],
      [
        {
          "style": [],
          "text": "eroii no\u015ftri urc\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "pe sc\u0103ri nemuritoare. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Din Mesele T\u0103cerii"
        }
      ],
      [
        {
          "style": [],
          "text": "prin Por\u0163ile Iubirii"
        }
      ],
      [
        {
          "style": [],
          "text": "Coloana nesf\u00eer\u015fit\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "se-nal\u0163\u0103 nemuririi, "
        }
      ],
      [
        {
          "style": [],
          "text": "iar nou\u0103 ne r\u0103m\u00eene"
        }
      ],
      [
        {
          "style": [],
          "text": "istoria fierbinte"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi sf\u00eenta datorie"
        }
      ],
      [
        {
          "style": [],
          "text": "s-o ducem \u00eenainte. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "La Poarta Sf\u00eentului S\u0103rut",
    "verses": [
      [
        {
          "style": [],
          "text": "Veni\u0163i cei ce-a\u0163i gustat Prezentul"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi Jertfa Marelui Trecut, "
        }
      ],
      [
        {
          "style": [],
          "text": "s\u0103 trecem spre Eternitate"
        }
      ],
      [
        {
          "style": [],
          "text": "prin Poarta Sf\u00eentului S\u0103rut. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Aici ne vom primi ve\u015fm\u00eentul"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-nc\u0103l\u0163\u0103mintea de azur"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi Semnul cunun\u0103rii noastre"
        }
      ],
      [
        {
          "style": [],
          "text": "cu Mirele Etern \u015fi Pur. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Aici ne-mbr\u0103\u0163i\u015feaz\u0103 Cerul"
        }
      ],
      [
        {
          "style": [],
          "text": "cel mai frumos \u015fi mai pl\u0103cut"
        }
      ],
      [
        {
          "style": [],
          "text": "cu cele patru Taine-a-Crucii"
        }
      ],
      [
        {
          "style": [],
          "text": "\u00een cel mai nesf\u00eer\u015fit S\u0103rut. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "De-aici ne-ncolon\u0103m pe drumul"
        }
      ],
      [
        {
          "style": [],
          "text": "eroilor spre Infinit"
        }
      ],
      [
        {
          "style": [],
          "text": "d\u00eend fiecare c\u00eete-o vam\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "de suflet \u015fi de trup sfin\u0163it. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "- C\u00eend confrunta\u0163i cu vreo furtun\u0103, "
        }
      ],
      [
        {
          "style": [],
          "text": "c\u00eend s\u00eengera\u0163i de vreun \u0163epu\u015f, "
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103 drumul unic spre cunun\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "e-acest sl\u0103vit \u015fi greu urcu\u015f. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "To\u0163i cei sfin\u0163i\u0163i prin Taina Mesei"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-a Por\u0163ii Sf\u00eentului S\u0103rut"
        }
      ],
      [
        {
          "style": [],
          "text": "p\u00een\u0103 la Treptele M\u0103ririi"
        }
      ],
      [
        {
          "style": [],
          "text": "pe-acest drum unic au trecut. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Comori nemuritoare s\u00eent",
    "verses": [
      [
        {
          "style": [],
          "text": "Comori nemuritoare s\u00eent"
        }
      ],
      [
        {
          "style": [],
          "text": "tot ce-au l\u0103sat p\u0103rin\u0163ii"
        }
      ],
      [
        {
          "style": [],
          "text": "acestui str\u0103mo\u015fesc p\u0103m\u00eent"
        }
      ],
      [
        {
          "style": [],
          "text": "privirilor \u015fi min\u0163ii"
        }
      ],
      [
        {
          "style": [],
          "text": "de la C\u0103min p\u00een-la Izvor"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi de la \u015ees la Munte"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u00eent numai st\u00eelpi l\u0103sa\u0163i de-un dor"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi loc sfin\u0163it de-o frunte. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "\u00cen orice grai e-un \u00een\u0163eles"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi pe-orice piatr\u0103-o raz\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "lucind cu veacurile-n mers"
        }
      ],
      [
        {
          "style": [],
          "text": "ca soarele-n amiaz\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "\u00cen\u0163elepciunea lor e-un bun"
        }
      ],
      [
        {
          "style": [],
          "text": "nepre\u0163uit de mare"
        }
      ],
      [
        {
          "style": [],
          "text": "pe care sufletul str\u0103bun"
        }
      ],
      [
        {
          "style": [],
          "text": "d\u0103 \u0163\u0103rii viitoare. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "... \u00centemeia\u0163i-v\u0103 profund"
        }
      ],
      [
        {
          "style": [],
          "text": "din anii tinere\u0163ii"
        }
      ],
      [
        {
          "style": [],
          "text": "pe-aceste semne ce ascund"
        }
      ],
      [
        {
          "style": [],
          "text": "divinul sens al vie\u0163ii"
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103-mbog\u0103\u0163indu-v\u0103 pe voi"
        }
      ],
      [
        {
          "style": [],
          "text": "cu-a lor etern\u0103-avere"
        }
      ],
      [
        {
          "style": [],
          "text": "v\u0103 ve\u0163i purta urma\u015fi eroi"
        }
      ],
      [
        {
          "style": [],
          "text": "spre ve\u015fnica-nviere. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Nemuritoarele Comori",
    "verses": [
      [
        {
          "style": [],
          "text": "Nemuritoarele Comori"
        }
      ],
      [
        {
          "style": [],
          "text": "ni-s m\u0103rturia vie"
        }
      ],
      [
        {
          "style": [],
          "text": "a celor ce nemuritori"
        }
      ],
      [
        {
          "style": [],
          "text": "s-au dus \u00een ve\u015fnicie"
        }
      ],
      [
        {
          "style": [],
          "text": "orice troi\u0163\u0103 \u015fi izvor"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi piatr\u0103 \u015fi c\u0103rare"
        }
      ],
      [
        {
          "style": [],
          "text": "ni-s semne ce ne spun de-a lor"
        }
      ],
      [
        {
          "style": [],
          "text": "vie\u0163i nemuritoare. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Acest sf\u00eent car cu boi trec\u00eend"
        }
      ],
      [
        {
          "style": [],
          "text": "pe drum t\u0103cut de sear\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-a dus prin veacuri leg\u0103n\u00eend"
        }
      ],
      [
        {
          "style": [],
          "text": "aceast\u0103 dulce \u0163ar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "e parc\u0103-n ultimul lui drum"
        }
      ],
      [
        {
          "style": [],
          "text": "pe-aceste dragi ponoare"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-l vom mai revedea de-acum"
        }
      ],
      [
        {
          "style": [],
          "text": "doar pe c\u0103r\u0103ri stelare. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Se duce \u015fi el c\u0103tre cer"
        }
      ],
      [
        {
          "style": [],
          "text": "pe calea cea duioas\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "acelor ce cu lerui ler"
        }
      ],
      [
        {
          "style": [],
          "text": "s-au dus pe veci acas\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "... veni\u0163i s\u0103 d\u0103m un sf\u00eent s\u0103rut"
        }
      ],
      [
        {
          "style": [],
          "text": "acestor semne sfinte"
        }
      ],
      [
        {
          "style": [],
          "text": "s\u0103 nu l\u0103s\u0103m acest Trecut"
        }
      ],
      [
        {
          "style": [],
          "text": "pe veci s\u0103 ne-nmorm\u00eente. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Din Preistorie coboar\u0103",
    "verses": [
      [
        {
          "style": [],
          "text": "Din Preistorie coboar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "acest Sf\u00eent Ciob\u0103na\u015f cu oi"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-n orice loc pe-aceast\u0103 \u0163ar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "e pus ceva din El \u015fi-n noi"
        }
      ],
      [
        {
          "style": [],
          "text": "at\u00eetea vitregiri de soart\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "Istoria ne-au chinuit"
        }
      ],
      [
        {
          "style": [],
          "text": "dar to\u0163i str\u0103mo\u015fii ce ne poart\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "prin moarte-au supravie\u0163uit. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "La orice strung\u0103-ntre p\u00eeraie"
        }
      ],
      [
        {
          "style": [],
          "text": "mereu ne moare-un Ciob\u0103nel"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-o dulce miori\u0163\u0103 laie"
        }
      ],
      [
        {
          "style": [],
          "text": "jelind se stinge dup\u0103 El"
        }
      ],
      [
        {
          "style": [],
          "text": "din plai \u00een plai, din st\u00een\u0103-n st\u00een\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi azi, cu-al veacurilor r\u00eend"
        }
      ],
      [
        {
          "style": [],
          "text": "M\u0103icu\u0163a Lui cu br\u00eeu de l\u00een\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "mereu ne moare \u00eentreb\u00eend. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Dar Moartea Lor devine Nunt\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "cu-ntregul Cosmos fericit"
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103ci Nedreptatea cea mai crunt\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "Divin Triumf Le-a-nve\u015fnicit"
        }
      ],
      [
        {
          "style": [],
          "text": "iar Ciob\u0103na\u015ful-F\u0103r\u0103-Moarte"
        }
      ],
      [
        {
          "style": [],
          "text": "mul\u0163indu-\u015ei Turma Lui de oi"
        }
      ],
      [
        {
          "style": [],
          "text": "prin veacuri trece mai departe"
        }
      ],
      [
        {
          "style": [],
          "text": "umpl\u00eend Istoria cu noi. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Troi\u0163\u0103 Maramure\u015fean\u0103",
    "verses": [
      [
        {
          "style": [],
          "text": "Troi\u0163\u0103 Maramure\u015fean\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "cu sf\u00eentul soarelui \u00eensemn"
        }
      ],
      [
        {
          "style": [],
          "text": "din c\u00eete-o lunc\u0103 \u015fi poian\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "mai vii cu vechiul t\u0103u \u00eendemn. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Cu fa\u0163a plin\u0103 de bl\u00eende\u0163e"
        }
      ],
      [
        {
          "style": [],
          "text": "de la r\u0103scruci"
        }
      ],
      [
        {
          "style": [],
          "text": "de la f\u00eent\u00eeni"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-nt\u00eempini \u015fi ne dai bine\u0163e"
        }
      ],
      [
        {
          "style": [],
          "text": "tot cum \u015ftiam de la b\u0103tr\u00eeni. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Azi v\u00eent \u015fi praf, tot mai afar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "te-alung\u0103 s\u0103 te-ascunzi \u00een mun\u0163i"
        }
      ],
      [
        {
          "style": [],
          "text": "unde t\u0103cerea mai coboar\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi unde se mai pleac\u0103 frun\u0163i. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Tu-i pl\u00eengi pe cei ce te alung\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "cum pl\u00eenge-o maic\u0103 dup\u0103 fii"
        }
      ],
      [
        {
          "style": [],
          "text": "c\u0103ci \u015fti ce greu o s\u0103-i ajung\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "durerea,\u00a0-\u00a0c\u00eend n-ai s\u0103 mai fii! "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "... Troi\u0163\u0103 Maramure\u015fean\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "nu ne l\u0103sa \u015fi nu te du"
        }
      ],
      [
        {
          "style": [],
          "text": "r\u0103m\u00eene \u0163ara prea orfan\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "atunci c\u00eend ni te duci \u015fi tu. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Cine tine-ar merge Miori\u0163a"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi Nani-na \u015fi Lerui ler"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi ni s-ar stinge lumini\u0163a"
        }
      ],
      [
        {
          "style": [],
          "text": "\u015fi-n cel mai t\u0103inuit ungher. "
        }
      ]
    ],
    "type": "poezie"
  },
  {
    "volume": "C\u00e2nt\u0103ri Nemuritoare",
    "book": "C\u00e2nt\u0103ri Nemuritoare vol. 3",
    "author": "Traian Dorz",
    "title": "Nemuritoarele comori",
    "verses": [
      [
        {
          "style": [],
          "text": "Nemuritoarele comori"
        }
      ],
      [
        {
          "style": [],
          "text": "de Lacrim\u0103 \u015fi Piatr\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "ne-au luminat din primii zori"
        }
      ],
      [
        {
          "style": [],
          "text": "str\u0103buna \u0163\u0103rii vatr\u0103"
        }
      ],
      [
        {
          "style": [],
          "text": "din Cozia la Vorone\u0163"
        }
      ],
      [
        {
          "style": [],
          "text": "din Putna la Tismana"
        }
      ],
      [
        {
          "style": [],
          "text": "comori de-at\u00eet de mare pre\u0163"
        }
      ],
      [
        {
          "style": [],
          "text": "ni-s leag\u0103nul \u015fi hrana. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce mari p\u0103rin\u0163i ne-au ctitorit"
        }
      ],
      [
        {
          "style": [],
          "text": "aceste-a\u015fez\u0103minte"
        }
      ],
      [
        {
          "style": [],
          "text": "cu ce m\u00eeini sfinte le-au zidit"
        }
      ],
      [
        {
          "style": [],
          "text": "pe cer \u015fi pe morminte"
        }
      ],
      [
        {
          "style": [],
          "text": "- eu n-au murit ci doar s-au dus"
        }
      ],
      [
        {
          "style": [],
          "text": "l\u0103s\u00eend Masa T\u0103cerii"
        }
      ],
      [
        {
          "style": [],
          "text": "prin Poarta de S\u0103rut \u00een Sus"
        }
      ],
      [
        {
          "style": [],
          "text": "pe Treptele-nvierii. "
        }
      ],
      "",
      [
        {
          "style": [],
          "text": "Ce grav e chipul lor t\u0103c\u00eend"
        }
      ],
